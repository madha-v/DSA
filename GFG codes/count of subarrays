// User function template for C++
class Solution {
  public:
    // #define ll long long

    ll countSubarray(int arr[], int n, int k) {
    ll total = 1LL * n * (n + 1) / 2; 
      ll valid=0;
      ll len=0;
      for(int i=0;i<n;i++)
      {
          if(arr[i]<=k)
          {
              len++;
            valid+=len;
          }
          else
          {
              len=0;
          }
      }
      return total-valid;
    }
};
